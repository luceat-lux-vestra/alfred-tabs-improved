{
  "description": "Search tabs for alfred workflow, aka alfred-tabs-improved",
  "homepage_url": "https://github.com/luceat-lux-vestra/alfred-tabs-improved",
  "manifest_version": 2,
  "name": "alfred-tabs@opera",
  "permissions": [
	"tabs"
  ],
  "icons": {
	"48": "icons/logo-48.png",
	"96": "icons/logo-96.png"
  },
  "background": {
	"scripts": [
	  "socket.io.js",
	  "background.js"
	]
  },
  "browser_action": {
	"browser_style": true,
	"default_title": "alfred-tabs@opera"
  },
  "version": "1.0"
}
